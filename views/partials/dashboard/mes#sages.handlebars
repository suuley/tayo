<div class="container">



    {{#if error}}
       {{#each error}}
        <div class="alert alert-danger alert-dismissible fade show mt-2 auto-hide" role="alert">
            <strong>{{error}}</strong>

            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
      {{/each}}
    {{/if}}
</div>
